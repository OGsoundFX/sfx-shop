<ul>
  <table>
    <th>
      <tr>Title</tr>
      <tr>Price</tr>
    </th>
    <tbody>
      <tr>
        <% @order.tracks.each do |track| %>
          <% displayed_track = SingleTrack.find(track) %>
          <li class="display-list"><%= displayed_track.title %> <%= humanized_money_with_symbol(displayed_track.price_cents / 100.to_f) %></li>
        <% end %>
      </tr>
    </tbody>
  </table>
</ul>
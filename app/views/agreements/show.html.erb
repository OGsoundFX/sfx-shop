<div class="padding-navbar content admin-min-height bg-dark pb-4">
  <div class="container d-flex justify-content-between mt-5">
    <div>
      <%= link_to :back, class: "submission-form__back-button" do %>
        <i class="fas fa-arrow-left mb-4"></i>
      <% end %>
    </div>
    <div style="height: 60px" class="">
      <%= link_to "View Seller Agreement (PDF)",
                rails_blob_path(@agreement.pdf, disposition: "inline"),
                target: "_blank",
                rel: "noopener",
                class: "btn btn-admin-active btn-lg" %>
    </div>
    <div></div>
  </div>
  <div class="container">
    <div class="agreement-body">
      <%= @agreement.body.to_s.html_safe %>
    </div>
  </div>
</div>

